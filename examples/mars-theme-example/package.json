{
  "name": "example-mars-theme",
  "version": "1.2.0",
  "private": true,
  "description": "Example of a project using mars-theme.",
  "scripts": {
    "dev": "frontity dev",
    "dev:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity dev",
    "build": "frontity build",
    "build:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity build",
    "serve": "frontity serve",
    "serve:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity serve",
    "now": "npx frontity build && npx now --token $NOW_TOKEN",
    "now:alias": "npx now alias --token $NOW_TOKEN mars-theme-$COMMIT_SHA.now.sh"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@frontity/core": "^1.4.1",
    "@frontity/html2react": "^1.1.15",
    "@frontity/mars-theme": "^1.3.0",
    "@frontity/tiny-router": "^1.0.18",
    "@frontity/wp-source": "^1.4.3",
    "babel-plugin-frontity": "^1.0.1",
    "frontity": "^1.4.3"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./build/static/mars-theme-example*.js",
        "compression": "none"
      }
    ]
  }
}
