    "regenerator-runtime": "^0.13.7",
    "require-from-string": "^2.0.2",
    "symlink-dir": "4.1.0",
    "ts-node": "^9.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
