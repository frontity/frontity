    "test:inspect": "node --inspect ./node_modules/.bin/jest --watch --no-cache --runInBand",
    "build": "tsc --project ./tsconfig.build.json",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/types": "^7.6.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "babel-plugin-tester": "^7.0.1",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3"
  }
}
