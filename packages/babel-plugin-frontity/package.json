{
  "name": "babel-plugin-frontity",
  "version": "1.0.2",
  "description": "Babel Plugin for Frontity",
  "keywords": [
    "frontity"
  ],
  "homepage": "https://frontity.org",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/frontity/frontity.git"
  },
  "bugs": {
    "url": "https://community.frontity.org"
  },
  "scripts": {
    "test:ci": "../../node_modules/.bin/jest --ci --coverage",
