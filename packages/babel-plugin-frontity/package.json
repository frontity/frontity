    "build": "tsc --project ./tsconfig.build.json",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/types": "^7.6.3"
  },
  "devDependencies": {
    "babel-plugin-tester": "^7.0.1"
