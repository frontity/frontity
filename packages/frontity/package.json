{
  "name": "frontity",
  "version": "1.0.6",
  "description": "Frontity cli and entry point to other packages",
  "keywords": [
    "frontity",
    "cli"
  ],
  "homepage": "https://frontity.org",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "frontity": "dist/commands.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontity/frontity.git"
  },
  "bugs": {
    "url": "https://community.frontity.org"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "node dist/commands.js",
    "dev": "ts-node src/commands.ts",
    "ts": "ts-node",
    "test": "jest --watch",
    "test:ci": "jest --colors",
    "build": "tsc --project ./tsconfig.build.json",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@frontity/connect": "^1.0.1",
    "@frontity/types": "^1.0.1",
    "@loadable/component": "^5.10.1",
    "chalk": "^2.4.2",
    "clipboardy": "^2.1.0",
    "commander": "^2.20.0",
    "envinfo": "^7.3.1",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.3.1",
    "node-fetch": "^2.4.1",
    "ora": "^3.4.0",
    "ramda": "^0.26.1",
    "react-helmet": "^5.2.1",
    "tar": "^4.4.8",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@frontity/core": "^1.1.1",
    "@types/fs-extra": "^5.0.5",
    "@types/inquirer": "^6.0.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "@types/node-fetch": "^2.3.3",
    "@types/phin": "^3.3.0",
    "@types/ramda": "^0.26.6",
    "@types/shelljs": "^0.8.4",
    "@types/stack-trace": "0.0.29",
    "@types/tar": "^4.0.0",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2"
  }
}
