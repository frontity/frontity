{
  "name": "frontity",
  "version": "0.1.4",
  "description": "Frontity cli and entry point to other packages",
  "keywords": [
    "frontity"
  ],
  "homepage": "https://frontity.org",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "frontity": "dist/commands.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontity/frontity.git"
  },
  "bugs": {
    "url": "https://community.frontity.org"
  },
  "scripts": {
    "start": "node dist/commands.js",
    "dev": "ts-node src/commands.ts",
    "test": "jest --watch --config jest.config.json",
    "test:ci": "jest --config jest.config.json --colors",
    "build": "tsc --project ./tsconfig.build.json",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@frontity/core": "^0.2.0",
    "@types/fs-extra": "^5.0.5",
    "@types/inquirer": "^6.0.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "@types/phin": "^3.3.0",
    "@types/ramda": "^0.26.6",
    "@types/shelljs": "^0.8.4",
    "@types/stack-trace": "0.0.29",
    "@types/tar": "^4.0.0",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "fs-extra": "^7.0.1",
    "ora": "^3.4.0",
    "phin": "^3.3.0",
    "ramda": "^0.26.1",
    "tar": "^4.4.8"
  }
}
